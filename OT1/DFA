digraph DFA {
	rankdir=LR
	S [color=blue]
	S -> A [label=a]
	S [color=blue]
	S -> B [label=b]
	A [color=black]
	A -> C [label=a]
	A [color=black]
	A -> B [label=b]
	B [color=black]
	B -> A [label=a]
	B [color=black]
	B -> D [label=b]
	C [color=red shape=doublecircle]
	C -> C [label=a]
	C [color=red shape=doublecircle]
	C -> E [label=b]
	D [color=red shape=doublecircle]
	D -> F [label=a]
	D [color=red shape=doublecircle]
	D -> D [label=b]
	E [color=red shape=doublecircle]
	E -> F [label=a]
	E [color=red shape=doublecircle]
	E -> D [label=b]
	F [color=red shape=doublecircle]
	F -> C [label=a]
	F [color=red shape=doublecircle]
	F -> E [label=b]
}
